$uhf
$alpha shells
 a       1-9                                    ( 1 )
$beta shells
 a       1-9                                    ( 1 )
$scfiterlimit      100
$scfconv        7
$thize     0.10000000E-04
$thime        5
$scfdamp   start=1.000  step=0.050  min=0.100
$scfdump
$scfintunit
 unit=30       size=0        file=twoint
