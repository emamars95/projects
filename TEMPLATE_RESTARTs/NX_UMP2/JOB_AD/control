   basis =h aug-cc-pVDZ                                                        \
   cbas  =h aug-cc-pVDZ
b  5                                                                           \
   basis =b aug-cc-pVDZ                                                        \
   cbas  =b aug-cc-pVDZ
$basis    file=basis
$rundimensions
   dim(fock,dens)=5574
   natoms=8
   nshell=48
   nbf(CAO)=104
   dim(trafo[SAO<-->AO/CAO])=112
   rhfshells=2
   nbf(AO)=100
$uhfmo_alpha   file=alpha
